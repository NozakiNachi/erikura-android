<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="handler"
            type="jp.co.recruit.erikura.presenters.fragments.PropertyNotesFragmentEventHandlers" />
        <variable
            name="viewModel"
            type="jp.co.recruit.erikura.presenters.fragments.PropertyNotesViewModel" />
    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".presenters.fragments.PropertyNotesFragment">
    <Button
        style="@style/tertiaryButton"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:text="@{viewModel.PropertyNotesButtonText}"
        app:onSafeClick="@{handler::onClickPropertyNotes}"/>


    </FrameLayout>
</layout>